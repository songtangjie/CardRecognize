<!--miniprogram/pages/home/childCpns/s-card-category/s-card-category.wxml-->

<view class="card-category">
  <block wx:for="{{categories}}" wx:key="unique">
    <s-card-item title="{{item.title}}" 
                 iconPath="{{item.icon}}"
                 bindtap="itemClick"
                 data-index="{{index}}"/>
  </block>
</view>
